<UserControl x:Class="BSTU.FileCabinet.WPF.Controls.AdminNavigationBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BSTU.FileCabinet.WPF.Controls"
             xmlns:nav="clr-namespace:BSTU.FileCabinet.WPF.State.Navigators"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="100">
    <Grid>
        <StackPanel Orientation="Vertical" VerticalAlignment="Center">
            <Button Content="Authorization" Command="{Binding UpdateCurrentViewModelCommand}" CommandParameter="{x:Static nav:ViewType.Authorization}"></Button>
            <Button Content="Faculty"></Button>
            <Button Content="Pulpit"></Button>
            <Button Content="Group"></Button>
            <Button Content="Speciality"></Button>
            <Button Content="Student"></Button>
            <Button Content="Subject"></Button>
            <Button Content="Teacher"></Button>
            <Button Content="TeacherSubject"></Button>
        </StackPanel>
    </Grid>
</UserControl>
